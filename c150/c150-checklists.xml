				<condition>
					<equals>
						<property>/surface-positions/flap-pos-norm</property>
						<value>0.0</value>
					</equals>
				</condition>
				<binding>
					<command>nasal</command>
					<script>
						controls.flapsDown( -1 );
						var ls = setlistener("/surface-positions/flap-pos-norm", func( i ){ 
								if ( i.getDoubleValue() == 0.0 ){ 
									controls.flapsDown( 0 );
									removelistener( ls );
								}
							});
					</script>
				</binding>
			</item>
