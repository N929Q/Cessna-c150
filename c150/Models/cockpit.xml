	<!-- key / magnetos / starter -->
	
	<animation>
		<type>knob</type>
		<object-name>Key</object-name>
		
		<expression>
			<sum>
				<product>
					<sum>
						<property>controls/engines/engine[0]/c150-magnetos</property>
						<property>controls/engines/engine[0]/starter-key</property>
					</sum>
					<value>30</value>
				</product>
				<value>-70</value>
			</sum>
		</expression>
		<axis>
			<x1-m> 0.48642</x1-m>
			<y1-m>-0.370194</y1-m>
			<z1-m>-0.000136885</z1-m>
			<x2-m> 0.37943</x2-m>
			<y2-m>-0.370193</y2-m>
			<z2-m>-0.0170814</z2-m>
		</axis>
		
		<action>
			<name>Engine Start</name>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[0]/starter-switch</property>
				<value type="bool">true</value>
				<condition>
					<greater-than>
						<property>controls/engines/engine[0]/c150-magnetos</property>
						<value>3</value>
					</greater-than>
				</condition>
			</binding>
			
			<binding>
				<command>property-adjust</command>
				<property>controls/engines/engine[0]/c150-magnetos</property>
				<factor>1</factor>
				<min>0</min>
				<max>4</max>
			</binding>
		</action>
		<release>
			<binding>
				<condition>
					<greater-than>
						<property>controls/engines/engine[0]/c150-magnetos</property>
						<value>3</value>
					</greater-than>
				</condition>
				<command>nasal</command>
				<script>setprop("/controls/engines/engine[0]/starter-switch", 0);
					setprop("controls/engines/engine[0]/c150-magnetos", 3);
				</script>
			</binding>
		</release>
		
		<hovered>
			<binding>
				<command>set-tooltip</command>
				<tooltip-id>magneto-switch</tooltip-id>
				<label>Magnetos: %s</label>
				<property>controls/engines/engine[0]/c150-magnetos</property>
				<mapping>nasal</mapping>
				<script>
					var m = arg[0];
					if (m == 1) return 'RIGHT';
					if (m == 2) return 'LEFT';
					if (m == 3) return 'BOTH';
					if (m == 4) return 'START !';
					return 'OFF';
				</script>
			</binding>
		</hovered>
	</animation>
	
