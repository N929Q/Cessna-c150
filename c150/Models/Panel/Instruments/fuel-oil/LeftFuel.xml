<?xml version="1.0"?>

<PropertyList>
	
	<path>LeftFuel.ac</path>

	<effect>
		<inherits-from>../../../Effects/interior/instrument-glass</inherits-from>
		<object-name>l_fuel.glass</object-name>
	</effect>
	
	<effect>
		<inherits-from>Aircraft/c150/Models/Effects/interior/flashlight</inherits-from>
		<object-name>l_fuel</object-name>
		<object-name>l_fuel.needle</object-name>
	</effect>
	
	<animation>
		<type>material</type>
		<object-name>l_fuel</object-name>
		<object-name>l_fuel.needle</object-name>
		<emission>
			<red>1.0</red>
			<green>0.2</green>
			<blue>0.0</blue>
			<factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
		</emission>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>l_fuel.needle</object-name>
		<property>instrumentation/fuel-indicator[0]/needle-deg</property>
		<factor>1</factor>
		<center>
			<z-m>-0.015</z-m>
		</center>
		<axis>
			<x>-1</x>
		</axis>
	</animation>
	
</PropertyList>
