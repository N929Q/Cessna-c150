<PropertyList>
	<path>preflight.ac</path>
	
	<animation>
		<type>select</type>
		<object-name>preflight.pull_nose_down</object-name>
		<object-name>preflight.suck_stall_vent</object-name>
		<object-name>preflight.oil</object-name>
		<object-name>preflight.fuel_strainer</object-name>
		<condition>
			<and>
				<property>sim/walker/outside</property>
				<property>gear/gear[0]/wow</property>
				<property>gear/gear[1]/wow</property>
				<property>gear/gear[2]/wow</property>
				<not>
					<property>sim/current-view/internal</property>
				</not>
			</and>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>tiedown.L.pick</object-name>
		<object-name>tiedown.R.pick</object-name>
		<object-name>tiedown.T.pick</object-name>
		<object-name>chocks.L.pick</object-name>
		<object-name>chocks.R.pick</object-name>
		<condition>
			<and>
				<property>gear/gear[1]/wow</property>
				<property>gear/gear[2]/wow</property>
				<not>
					<property>sim/current-view/internal</property>
				</not>
			</and>
		</condition>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>preflight.pull_nose_down</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>controls/preflight/pull-nose-down</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/preflight/pull-nose-down</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>preflight.suck_stall_vent</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>controls/preflight/suck-stall-warning</property>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<command>property-assign</command>
					<property>controls/preflight/suck-stall-warning</property>
					<value>0</value>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>select</type>
		<object-name>preflight.oil</object-name>
		<object-name>preflight.fuel_strainer</object-name>
		<condition>
			<property>engines/engine[0]/complex-engine-procedures</property>
		</condition>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>preflight.oil</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>dialog-show</command>
				<dialog-name>c150-oil</dialog-name>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>preflight.fuel_strainer</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>dialog-show</command>
				<dialog-name>c150-fuel-strainer</dialog-name>
			</binding>
		</action>
	</animation>
	
	<!-- Tie-Downs -->
	<animation>
		<type>pick</type>
		<object-name>tiedown.L.pick</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/c150/tiedowns/left/attached</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>tiedown.R.pick</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/c150/tiedowns/right/attached</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>tiedown.T.pick</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/c150/tiedowns/tail/attached</property>
			</binding>
		</action>
	</animation>
	<model>
		<name>tiedownL</name>
		<path>Ground/tiedown/tiedown.xml</path>
		<overlay>
			<params>
				<heading-deg>sim/model/c150/tiedowns/left/heading-deg</heading-deg>
				<pitch-deg>sim/model/c150/tiedowns/left/pitch-deg</pitch-deg>
				<length-m>sim/model/c150/tiedowns/left/length</length-m>
				<visible>sim/model/c150/tiedowns/left/attached</visible>
			</params>
		</overlay>
		<offsets>
			<x-m>  0.7  </x-m>
			<y-m> -1.97 </y-m>
			<z-m>  0.46 </z-m>
			<heading-deg>-90.0</heading-deg>
		</offsets>
	</model>
	<model>
		<name>tiedownR</name>
		<path>Ground/tiedown/tiedown.xml</path>
		<overlay>
			<params>
				<heading-deg>sim/model/c150/tiedowns/right/heading-deg</heading-deg>
				<pitch-deg>sim/model/c150/tiedowns/right/pitch-deg</pitch-deg>
				<length-m>sim/model/c150/tiedowns/right/length</length-m>
				<visible>sim/model/c150/tiedowns/right/attached</visible>
			</params>
		</overlay>
		<offsets>
			<x-m>  0.7  </x-m>
			<y-m>  1.97 </y-m>
			<z-m>  0.46 </z-m>
			<heading-deg> 90.0</heading-deg>
		</offsets>
	</model>
	<model>
		<name>tiedownT</name>
		<path>Ground/tiedown/tiedown.xml</path>
		<overlay>
			<params>
				<heading-deg>sim/model/c150/tiedowns/tail/heading-deg</heading-deg>
				<pitch-deg>sim/model/c150/tiedowns/tail/pitch-deg</pitch-deg>
				<length-m>sim/model/c150/tiedowns/tail/length</length-m>
				<visible>sim/model/c150/tiedowns/tail/attached</visible>
			</params>
		</overlay>
		<offsets>
			<x-m>  5.125 </x-m>
			<y-m>  0.0   </y-m>
			<z-m> -0.357 </z-m>
			<heading-deg>180.0</heading-deg>
		</offsets>
	</model>
	<!-- Wheel Chocks -->
	<animation>
		<type>pick</type>
		<object-name>chocks.L.pick</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/c150/chocks/left-enable</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>chocks.R.pick</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/c150/chocks/right-enable</property>
			</binding>
		</action>
	</animation>
</PropertyList>
